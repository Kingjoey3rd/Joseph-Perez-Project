#include <iostream>

using namespace std;

double GRADESAverage(double GRADES[], int numGRADES) {
    double TOTAL = 0;
    for (int i = 0; i < numGRADES; ++i) {
        TOTAL += GRADES[i];
    }
    return TOTAL / numGRADES;
}

int main() {
    int N;
    cout << "Please enter number of grades that : ";
    cin >> N;
    cout << "Enter Here " << N << " grades: "<<endl;
    double *GRADES = new double[N];
    for (int i = 0; i < N; ++i) {
        cin >> GRADES[i];
    }
    cout << "Grade average going to be " << GRADESAverage(GRADES, N) << endl;
    delete[] GRADES;
    return 0;
}
