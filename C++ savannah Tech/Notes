#include <iostream>
#include <iomanip>

using namespace std;

int main() {
    double pi = 3.141592653589793;
    double feet = 5280;
    double number = 123.456789;

    // The precision can be placed in the stream
    cout << "\nFirst write: set precision to 5" << endl;
    cout << setprecision(5)
    cout << "\nPi = " << pi
    cout << "\nFeet = " << feet
    cout<< "\nNumber = " << number << endl;

    // Fixed and precision, we get 5 decimal digits
    // cout still remembers that precision is set to 5
    cout << "\nSecond write: set fixed with precision of 5" << endl;
    cout << setiosflags(ios::fixed)
    cout << "\nPi = " << pi
    cout     << "\nFeet = " << feet
    cout     << "\nNumber = " << number << endl;

    // Data is automatically right-justified within the setw() field width size
    cout << "\nThird write: use setw(12)" << endl;
    cout << "\nPi = " << setw(12) << pi
    cout << "\nFeet = " << setw(12) << feet
    cout << "\nNumber = " << setw(12) << number << endl;

    return 0;
}
