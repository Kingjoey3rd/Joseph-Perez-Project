#include <iostream>
#include <fstream>
#include <iomanip>
using namespace std;


float FindAve(float grades[], int total);

int main ()

{
    ifstream input;
    ofstream output;

    input.open("StudentGrades.txt");
    output.open("Grades.txt");

    int totalGrades=8;
    float grade [8];
    char name[25];
    float sum=0, average;

    if (!input)
        {
    cout << "File did not open correctly"<< endl;
    return 0;
        }
    output<<"The C++ Grading Program"<<endl;
    output<<"Student Name           Average Grade \n";

    output.precision(3);
    output.setf(ios::fixed);

    while (input)
        {

    input.get(name,25);
    for (int x=0; x<8; ++x)

        {

        input>>grade[x];
        }

    input.ignore();

        average = FindAve (grade,totalGrades);

        output<<name<<setw(8)<<average<<endl;
        }
    output<<"All Done!";

    input.close();
    output.close();

    return 0;
}


float FindAve(float grades[],int totalGrades)
{
    float sum = 0.0;
    int x;
    for(x = 0; x<totalGrades; ++x)
    {
        sum = sum + grades[x];
    }
    return (sum/totalGrades);
}
