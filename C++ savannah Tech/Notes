21.
#include <iostream>
#include <string>

using namespace std;

#define PI 3.14159265

int main()
{
    
    double AreaofSufaceA, AreaofSufaceB, VolumeofsphereA,VolumeofsphereB, RadiusA, RadiusB;
    string Golfball, Baseball;
    
    cout<<"Value of Radius A = "<<Golfball<<endl;
    cin>>RadiusA;
    cout<<"Value of Radius B = "<<Baseball<<endl;
    cin>>RadiusB;
    
    AreaofSufaceA=4*PI*(RadiusA*RadiusA);
    AreaofSufaceB=4*PI*(RadiusB*RadiusB);
    
    VolumeofsphereA=RadiusA*RadiusA*RadiusA*4/3*PI;
    VolumeofsphereB=RadiusB*RadiusB*RadiusB*4/3*PI;
    
    cout.setf(ios::showpoint | ios::fixed);	
    cout.precision(2);
    cout<<endl;
    cout<<"Sphere Equation!!!"<<endl;
    
    cout<<endl;
    
    cout<<"Volume of SphereA  = "<<VolumeofsphereA<<endl;
    cout<<"Volume of SphereB  = "<<VolumeofsphereB<<endl;
    cout<<endl;
    
    cout<<"Area of SphereA: "<<AreaofSufaceA<<endl;
    cout<<"Area of SphereB: "<<AreaofSufaceB<<endl;
    
    return 0;
}
