68.
#include <iostream>
#include <fstream> 
#include <cmath>
using namespace std;

int main()
{
    ifstream input;
    ofstream output; 

    input.open("Values for Resistors.txt");
    output.open("Total Resistance.txt");

    float Resistors1, Resistors2, Resistors3, Resistors4;
    input>>Resistors1;
    input>>Resistors2;
    input>>Resistors3;
    input>>Resistors4;

    float Rt_A1=Resistors1+Resistors2+Resistors3+Resistors4;
    output<<"The total resistance for a series circuit going to be: "<<Rt_A1<<endl;

    float Rt_B=(1/Resistors1)+ (1/Resistors2)+(1/Resistors3)+ (1/Resistors4);
    float Rt_B1=pow(Rt_B,-1);
    output<<"The total resistance for a parallel circuit going to be: "<<Rt_B1<<endl;

    input.close();
    output.close();

    return 0;
}
