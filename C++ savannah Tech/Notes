43.
#include <iostream>
using namespace std;
 
int main()
{
    int n=34;
    int r=n; // "r" has the same value as "n" but will not have the same address

    int a=12;
    int &b=a; // assigning "&b" is a reference to "a" with while holding the same address

    cout<<"n= "<<n<<endl;
    cout<<"&n= "<<&n<<endl;
    cout<<"r= "<<r<<endl;
    cout<<"&r= "<<&r<<endl;

    cout<<endl;

    cout<<"a= "<<a<<endl;
    cout<<"&a= "<<&a<<endl;
    cout<<"b= "<<b<<endl;
    cout<<"&b= "<<&b<<endl;

  return 0;
}
