#include<iostream>
using namespace std;

int main()
{
   int arr[10];
   cout<<"\nEnter the values of array\n"; 
   for(int i=0;i<10;i++)
   {
       cout<<"Please enter an integer : ";
       cin>>arr[i];
    
   }

   cout<<"\nThe values in the original array are : \n";
   for(int i =0;i<10;i++)
   cout<<arr[i]<<" ";

   
   for(int i=0,j=9;i<j;i++,j--)
   {
       int t=arr[i];
       arr[i]=arr[j];
       arr[j]=t;
   }

   cout<<"\nThe values in the Reverse array are : \n";
   for(int i =0;i<10;i++)
   cout<<arr[i]<<" ";

   return 0;
}
