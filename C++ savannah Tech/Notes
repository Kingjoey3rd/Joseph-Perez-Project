#include <iostream>
#include <string>

using namespace std;

int main() {
    string answer;
    int R1, R2, Rt;

    do {
        cout << "Enter 2 resistor values in ohms: " << endl;
        cin >> R1 >> R2;
        Rt = R1 + R2;
        cout << "\nTotal resistance = " << Rt << " ohms" << endl;
        cout << "Would you like to continue? (yes/no): ";
        cin.ignore();
        getline(cin, answer);
    } while (answer == "yes");

    cout << "Thanks!" << endl;

    return 0;
}
//---------------------------------------------
#include <iostream>

using namespace std;

int main() {
    int n, f = 1;
    cout << "Please enter the number you want to find the factorial of: " << endl;
    cin >> n;

    int temp = n;  // Store the original value of n to use it in the loop

    do {
        f = f * temp;
        temp--;
    } while (temp > 1);

    cout << "The factorial of " << n << " is " << f << endl;

    return 0;
}
