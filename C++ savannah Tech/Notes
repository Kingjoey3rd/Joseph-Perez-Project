54.
#include <iostream>

using namespace std;

void WRITEHELLOWORLD (int row, int col);

int main()
{
    cout << "Pick any number of rows: ";
    int row;
    cin  >> row;
    cout << "Pick any number of columns: ";
    int col;
    cin  >> col;
    WRITEHELLOWORLD(row,col);
}    
    
void WRITEHELLOWORLD (int row, int col)
{
    if (row!=0 && col!=0)
    {
         for(int X = 0; X < row; ++X)
    {
        for(int H = 0; H < col; ++H)
            cout << "Hello world !!" << ' ';
