using namespace std;
class Resistors
{
    public:
    double ResistorsA,ResistorsB, Rt_B1,Rt_A1;
    void input()
    {
        cout<<"Enter the Resistors1 for here: "<<endl;
        cin>>ResistorsA;
        cout<<"Enter the Resistors2 for here: "<<endl;
        cin>>ResistorsB;
    }
    void findseries()
    {
        Rt_A1=ResistorsA+ResistorsB;
    }
      void findparallel()
    {
       Rt_B1=1/(1/ResistorsA+1/ResistorsB);
    }
    void displayA()
    {
        cout<<"The total resistance for a series circuit is: "<<Rt_A1<<endl<<endl;
    }
    void displayB()
    {
        cout<<"The total resistance for a parallel circuit is: "<<Rt_B1<<endl<<endl;
    }
};
int main()
{
    Resistors Resistors1;
    Resistors1.input();
    Resistors1.findseries();
    Resistors1.displayA();
   
    Resistors Resistors2;
    Resistors2.input();
    Resistors2.findparallel();
    Resistors2.displayB();

    return 0;
}
