26.#include <iostream>
#include <cmath>
using namespace std;

#define PI 3.14159265

int main()
{
    float Inner_of_circleR, Outter_of_circleR, Inner_Area, Outter_Area, Inner_Circumference, Outter_Circumference;
    cout<<"ENTER THE RADIUS OF THE INNER OF THE CIRCLE: "<<endl;
    cin>>Inner_of_circleR;
    cout<<"ENTER THE RADIUS OF THE OUTTER OF THE CIRCLE: "<<endl;
    cin>>Outter_of_circleR;
    Inner_Area=pow(Inner_of_circleR,2)*PI;
    Outter_Area=pow(Outter_of_circleR,2)*PI;
    Inner_Circumference=2*PI*Inner_of_circleR;
    Outter_Circumference=2*PI*Outter_of_circleR;
    cout.setf(ios::showpoint | ios::fixed);
    cout.precision(2);
    cout<<endl<<endl;
    cout<<"Intro Circle"<<endl<<endl;
    cout<<"Area of the Circle! "<<endl<<endl;
    cout<<"Inner Area of the Circle = "<<Inner_Area<<endl;
    cout<<"Outter Area of the Circle = "<<Outter_Area<<endl;
    cout<<endl;
    cout<<"Circumference of the Circle! "<<endl<<endl;
    cout<<"Circumference of Inner Circle = "<<Inner_Circumference<<endl;
    cout<<"Circumference of Inner Circle = "<<Outter_Circumference<<endl;
    
    return 0;
   
}
