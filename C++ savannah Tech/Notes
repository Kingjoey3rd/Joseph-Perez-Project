#include <iomanip>
using namespace std;

int main()
{
    char char1, char2, char3;
    int nChar1, nChar2, nChar3;
    
    cout<<"Enter three characters: "<<endl;
    cin>>char1>>char2>>char3;
    
    cout<<"\n\n Your Characters: "<<setw(5) <<char1<<setw(5)<<char2<<setw(5)<<nChar3;
    
    nChar1 = static_cast<int>(char1);
    nChar2 = static_cast<int>(char2);
    nChar3 = static_cast<int>(char3);
    
    cout<< "\n Decimal From: "<<setw(5)<<nChar1<<setw(5)<<nChar2<<setw(5)<<nChar3;
    
    cout.unsetf(ios::dec);
    cout.setf(ios::oct);
    cout << "\n Octal form; "<<setw(5)<<nChar1<<setw(5)<<nChar2<<setw(5)<<nChar3;
    
    cout.unsetf(ios::oct);
    cout.setf(ios::hex);
    cout<< "\n Hex form: "<<setw(5)<<nChar1<<setw(5)<<nChar2<<setw(5)<<nChar3<<endl;
