50.
#include <iostream>
#include <string>
using namespace std;

void GRADES (string EXAM[], float GRADES[]);
float Avg (float GRADES[], float &SUM);

void GRADES (string EXAM[], float GRADES[])
{
    for(int i=0;i<4;++i)
    {
        cout<<"Please input grade for "<<EXAM[i]<<": "<<endl;
        cin>>GRADES[i];
    }
}

float Avg (float GRADES[], float &SUM)
{
    SUM=0.0;
    for(int i=0;i<4;++i)
    {
        SUM=SUM+GRADES[i];
    }

    return SUM/4.0;
}

int main()
{
    float grade_score [4];
    string EXAM[4]={"EXAM 1","EXAM 2","EXAM 3","EXAM 4"};
    float SUM, AVG;
    GRADES (EXAM, grade_score);
    AVG= Avg(grade_score, SUM);
    cout<<SUM<<endl;
    cout<<AVG<<endl;
    return 0;
}
