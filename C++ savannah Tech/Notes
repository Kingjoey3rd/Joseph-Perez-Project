#include <iostream>

using namespace std;
int max(int x, int y, int z);

int main()
{
    int x,y,z;
    do
    {
    cout<<"Please enter three positive intergers"<<endl;
    cin>>x>>y>>z;
    cout<<max(x,y,z)<<endl;
    } while (x!=0);
    
    return 0;
}
int max(int num1, int num2, int num3)
{
    if (num1<num2)
    {
        return num3;
    }
    else if (num2<num3)
    {
        return num1;
    }
    else 
    {
        return num2;
    }
}
