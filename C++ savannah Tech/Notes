20.
#include <iostream>
using namespace std;
 
int main() 
{
   int m = 44, n=66;
   int b, B, c, C ;

   cout<<"This is A now!!! "<<m<<endl;
 
   // Value of a will not be increased before assignment.
   b = ++m;
   cout << "Line 1 - Value of m is :" << b << endl;


   B =m++;
   cout << "Line 2 - Value of m is :" << B << endl ;


   cout << "Line 3 - Value of m is :" << m << endl ;

   cout<<endl;

   cout<< "This is the second part: "<<endl;
   cout<<endl;

   cout<<"This is N now!!! "<<n<<endl;
 
   // Value of a will not be increased before assignment.
   c = --n;
   cout << "Line 4 - Value of n is :" << c << endl;


   C =n--;
   cout << "Line 5 - Value of n is :" << C << endl ;


   cout << "Line 6 - Value of n is :" << n << endl ;
