72.
#include <iostream>
#include <cstring> 
#include <string>  
#include <fstream> 
using namespace std;

int main() 
{
    fstream inputFile;
    fstream outputFile;
    inputFile.open("studentgrades.txt", ios::in);
    outputFile.open("Grades.txt", ios::out);
    if(!inputFile || !outputFile) {
        return 0;
    }
    string line;
    string names[100];
    float grades[100];
    int lineCounter = 0;
    while (getline(inputFile, line, '\n')) {
        int n = line.length();
        char char_array[n + 1];
        strcpy(char_array, line.c_str());
        string expectedLine;
        char lastChar = ' ';
        for (int i = 0; i < n; i++) {
            char thisChar = char_array[i];

            if(thisChar != ' ' || (lastChar != ' ' and thisChar == ' ')) {
                expectedLine = expectedLine + thisChar;
                lastChar = thisChar;
            }
            
        }

        size_t pos = 0;
        string word;
        string name = "";
        float totalgrade = 0;
        int wordCount = 0;

        while (true) {
 
            pos = expectedLine.find(' ');

            word = expectedLine.substr(0, pos);

            expectedLine.erase(0, pos + 1);
            if (wordCount ==0) 
            {
                name = name +word;
           
            } 
            else if (wordCount ==1)
            {
                name = name + " "+word;
            } 
            else 
            {
                //totalgrade += stoi(word);
                
            }
            wordCount++;
            if(pos == string::npos) 
            {
                break;
            }
        }
        names[lineCounter] = name;
        grades[lineCounter] = totalgrade/ (wordCount-2);
        lineCounter++;
    }
    inputFile.close();


    outputFile << endl;
    outputFile << "  The C++ Grading Program  " << endl;
    outputFile << endl;
    char buffer[100];
        sprintf(buffer, "%-20s %-20s\n", "Student Name", "Average Grade");
        outputFile << buffer;
        for(int i = 0; i < lineCounter; i++) {
                string name = names[i];
                float grade = grades[i];
                sprintf(buffer, "%-23s %-20.3f\n", name.c_str(), grade);
                outputFile << buffer; 
        } 
    outputFile << endl;
    outputFile << " All Done!";           
    outputFile << endl;
    outputFile.close();
}
