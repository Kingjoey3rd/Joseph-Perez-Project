15.problem 4.
#include <iostream>
#include <cmath>
using namespace std;

int main()
{
   int menu_option, VR_1,VR_2,VR_3,VR_4, I;
   float resistor_1, resistor_2, resistor_3, resistor_4, Rt_A, Rt_B, Rt_C, Rt_B1;
   VR_1= 2, VR_2= 4, VR_3=2, VR_4=2, I=1;
   cout<<"Please enter the value of resistor 1: "<<endl;
   cin>>resistor_1;
   cout<<"Please enter the value of resistor 2: "<<endl;
   cin>>resistor_2;
   cout<<"Please enter the value of resistor 3: "<<endl;
   cin>>resistor_3;
   cout<<"Please enter the value of resistor 4: "<<endl;
   cin>>resistor_4;
   cout<<"Select one of the menu options between 1 to 4"<<endl;
   cin>>menu_option;

   Rt_A=resistor_1+resistor_2+resistor_3+resistor_4;
   Rt_B=(1/resistor_1)+ (1/resistor_2)+ (1/resistor_3)+ (1/resistor_4);
   Rt_B1=pow(Rt_B,-1);
   Rt_C=(I*VR_1)+(I*VR_2)+(I*VR_3)+(I*VR_4);

   switch (menu_option)
   {
    case 1:
    cout<<"The total resistance for a series circuit is: "<<Rt_A<<endl;
    break;
    case 2:
    cout<<"The total resistance for a parallel circuit is: "<<Rt_B1<<endl;
    break;
    case 3:
    cout<<"For the current series circuit, the total resistance is: "<<Rt_C<<endl;
    break;
    case 4:
    cout<<"Thank you for using the circuit program"<<endl;
    break;
    default:
    cout<<"Error, not a menu option"<<endl;
   }

    return 0;
}

